<van-toast id="van-toast-monthlyCard" />
<!-- 显示原价 -->
<van-card wx:if="{{!usedNewUserToken}}" title-class="title-class" thumb-class="thumb-class" desc-class="desc-class" price-class="price-class" num="1" price="{{totalFeeStr}}" desc="" title="月卡" thumb="https://6465-dev-3g5cl9ca65d64f4b-1324237307.tcb.qcloud.la/oS-PG64WoFk4zbqnPZQqrnLyms6U1708229118542.png?sign=3b93ca6759b00978a0fce25a0e2a0db8&t=1708422812" />

<!-- 显示折扣价 -->
<van-card wx:if="{{usedNewUserToken}}" title-class="title-class" thumb-class="thumb-class" desc-class="desc-class" price-class="price-class" num="1" origin-price="{{originalTotalFeeStr}}"  price="{{totalFeeStr}}" desc="" title="月卡" thumb="https://6465-dev-3g5cl9ca65d64f4b-1324237307.tcb.qcloud.la/oS-PG64WoFk4zbqnPZQqrnLyms6U1708229118542.png?sign=3b93ca6759b00978a0fce25a0e2a0db8&t=1708422812" />

<view class="checkbox-buttom__view">
  <van-checkbox value="{{ checked }}" bind:change="onChange" custom-class="checkbox-custom">确认接受《购卡协议》</van-checkbox>
</view>


<view wx:if="{{ checked }}" >
  <van-submit-bar price="{{ totalFee }}" button-text="微信支付" bind:submit="onSubmit" tip="请先阅读并同意《购卡协议》"
  tip-icon="info-o"/>
</view>

<view wx:if="{{ !checked }}" >
  <van-submit-bar price="{{ totalFee }}" button-text="微信支付" disabled  tip="请先阅读并同意《购卡协议》"
  tip-icon="info-o"/>
</view>